<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="project.mall.orders.model.MallOrderTask" table="T_MALL_ORDER_TASK">
        <id name="id" type="java.lang.String">
            <column name="UUID" />
            <generator class="uuid.hex" />
        </id>

        <property name="orderOn"  type="java.lang.String">
            <column name="ORDER_ON" />
        </property>

        <property name="orderType"  type="java.lang.Integer">
            <column name="ORDER_TYPE" />
        </property>

        <property name="goodId"  type="java.lang.String">
            <column name="GOOD_ID" />
        </property>

        <property name="goodSku" type="java.lang.String">
            <column name="GOOD_ID_SKU" />
        </property>

        <property name="goodCount" type="java.lang.Integer">
            <column name="GOOD_COUNT" />
        </property>
        <property name="shopId" type="java.lang.String">
            <column name="SHOP_ID" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="STATUS" />
        </property>
        <property name="taskTime" type="timestamp">
            <column name="TASK_TIME" />
        </property>
        <property name="createTime" type="timestamp">
            <column name="CREATE_TIME" />
        </property>

        <property name="addressId" type="java.lang.String">
            <column name="ADDRESS_ID" />
        </property>

        <property name="partyId" type="java.lang.String">
            <column name="PARTY_ID" />
        </property>

    </class>
</hibernate-mapping>
